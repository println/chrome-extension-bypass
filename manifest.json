{
  "manifest_version": 2,
  "name": "Canvas Bypass",
  "version": "2.0.1",
  "description": "Bypass para carregar conte√∫dos ocultos",
  "content_scripts": [
    {
      "js": [
        "functions/anonymize.js",
        "functions/button.js",
        "handlers/submissions.js",
        "handlers/quizzes.js",
        "main.js"
      ],
      "matches": [
        "*://*.instructure.com/courses/*/assignments/*/submissions/*",
        "*://*.instructure.com/courses/*/quizzes/*"
      ],
      "run_at": "document_end"
    }
  ]
}
